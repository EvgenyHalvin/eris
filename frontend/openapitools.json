{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.4.0",
    "generators": {
      "v1.0": {
        "generatorName": "typescript-axios",
        "inputSpec": "./api.json",
        "output": "./src/shared/api/axios-client",
        "additionalProperties": {
          "supportsES6": true,
          "useSingleRequestParameter": true
        }
      }
    }
  }
}
